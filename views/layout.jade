doctype 5
html(ng-app="music", ng-controller='HomeCtrl')
  head
    title {{artist.name}} MP3 descargar musica GRATIS
    meta(name='fragment', content='!')
    meta(name='Description', content='{{artist.name}} MP3 descargar musica GRATIS. Escuchar musica de {{artist.name}} online')
    meta(name='keywords', content='{{artist.name}}, descargar, musica, gratis, mp3 {{artist.name}}, musica {{artist.name}}, videos, free, mp3, bajar mp3, descargar mp3, escuchar musica {{artist.name}}')
    script(src='http://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js')
    script(src='http://ajax.googleapis.com/ajax/libs/jqueryui/1/jquery-ui.min.js')
    script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.1.5/angular.min.js')
    script(src='http://cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.6.0/ui-bootstrap-tpls.js')
    script(src='js/helpers.js') 
    script(src='js/angular-audio-player.js') 
    script(src='js/angular-dragdrop.min.js') 
